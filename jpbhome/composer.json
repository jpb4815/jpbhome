{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.7.2@stable",
        "silverstripe/login-forms": "4.3.0@stable",
        "silverstripe/blog": "^3.2",
        "undefinedoffset/sortablegridfield": "^2.0",
        "wildbit/postmark-php": "^2.5",
        "google/recaptcha": "^1.1",
        "kinglozzer/metatitle": "^2.0.3",
        "symfony/filesystem": "3.4.9",
        "silvershop/silverstripe-hasonefield": "2.1.1",
        "silverstripe/redirectedurls": "dev-master"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "platform": {
            "php": "7.2.22"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
